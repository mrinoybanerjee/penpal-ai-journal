{
    "manifest_version": 3,
    "name": "My History Fetcher",
    "version": "1.0",
    "permissions": [
      "history",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "48": "icon.png"
    }
  }
  
  